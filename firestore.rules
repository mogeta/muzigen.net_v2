rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
  	match /blog_contents/{blog} {
      allow read: if true;
    }
    match /{document=**} {
      //allow read: if true; 
      allow write: if request.auth != null && request.auth.uid == "L0fcqFOnjAfYG8vWfErUGBrB33t2";
    }
  }
}